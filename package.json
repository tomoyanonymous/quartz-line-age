{
  "name": "quartz-line-age",
  "version": "1.1.5",
  "description": "A Quartz plugin that displays line authoring age visualization similar to Obsidian Git plugin's Line Authoring feature",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/line-age.css": "./src/line-age.css"
  },
  "type": "module",
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc && cp src/line-age.css dist",
    "test": "echo \"No tests yet\" && exit 0"
  },
  "keywords": [
    "quartz",
    "plugin",
    "git",
    "line-authoring"
  ],
  "author": "Tomoya Matsuura",
  "license": "MIT",
  "devDependencies": {
    "@types/hast": "^3.0.4",
    "@types/mdast": "^4.0.4",
    "@types/node": "^20.19.24",
    "hast": "^1.0.0",
    "mdast-util-find-and-replace": "^3.0.2",
    "rehype-parse": "^9.0.1",
    "rehype-stringify": "^10.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "typescript": "^5.0.0",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0"
  },
  "dependencies": {
    "rehype-raw": "^7.0.0",
    "rehype-slug": "^6.0.0"
  }
}
